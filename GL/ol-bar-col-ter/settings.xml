<settings>
  <servers>
    <server>
      <id>gitlab-maven-interface</id>
      <configuration>
        <httpHeaders>
          <property>
            <name>Private-Token</name>
            <value>${CI_SETTINGS}</value>
          </property>
        </httpHeaders>
      </configuration>
    </server>
    <server>
      <id>gitlab-maven-publish</id>
      <configuration>
        <httpHeaders>
          <property>
            <name>Job-Token</name>
            <value>${CI_JOB_TOKEN}</value>
          </property>
        </httpHeaders>
      </configuration>
    </server>
  </servers>
</settings>